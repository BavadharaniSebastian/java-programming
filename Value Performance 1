import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner A=new Scanner(System.in);
        int a=A.nextInt();
        float b=A.nextFloat();
        System.out.println(a);
        System.out.printf("%.2f",b);
    }
}
    
